<h1>Forum</h1>

<div id="forum-container">
  <% @wall_posts.each do |wall_post| %>
      <div class="post-div">
        <p class="post-header"><%= wall_post.user.first_name %> says...</p>
        <p class="post-date"><%= wall_post.created_at %></p>

        <p><%= wall_post.body %></p>
      </div>
  <% end %>
</div>



<br/>

<%= link_to 'New Wall post', new_wall_post_path %>
